{
  "name": "nansen-api-skill",
  "version": "1.0.0",
  "description": "Full-featured Nansen CLI for autonomous trading agents - API + MCP",
  "type": "module",
  "main": "dist/trader.js",
  "types": "dist/trader.d.ts",
  "exports": {
    ".": "./dist/trader.js",
    "./trader": "./dist/trader.js",
    "./agent": "./dist/agent.js",
    "./api": "./dist/api.js",
    "./mcp": "./dist/mcp.js",
    "./cache": "./dist/cache.js",
    "./rate-limiter": "./dist/rate-limiter.js",
    "./signal-log": "./dist/signal-log.js",
    "./types": "./dist/types.js"
  },
  "bin": {
    "nansen-api": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:api": "node scripts/test-api.mjs",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "nansen",
    "blockchain",
    "analytics",
    "smart-money",
    "defi",
    "trading",
    "openclaw"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
